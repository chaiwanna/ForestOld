rcesContent":["<template>\n  <div class=\"register container\">\n    <div class=\"row\">\n      <div class=\"col-md-3\"></div>\n      <div class=\"col-md-6\">\n        <h1 class=\"name-system\">ระบบลงทะเบียนผู้เข้าใช้พื้นที่เขตรักษาพันธุ์สัตว์ป่าเวียงลอ</h1>\n        <div class=\"card\">\n          <h1 class=\"card-header\">LOGIN</h1>\n          <div class=\"card-body\">\n            <form @submit.prevent=\"onSubmit()\">\n              <div class=\"form-group row\">\n                <label for=\"inputmobile\">เบอร์โทรศัพท์</label>\n\n                <input type=\"text\" v-model.trim=\"form.TEL\" class=\"form-control\" />\n              </div>\n\n              <div class=\"form-group row\">\n                <label for=\"inputpassword\">รหัสผ่าน</label>\n\n                <input type=\"password\" v-model.trim=\"form.PASSWORD\" class=\"form-control\" />\n              </div>\n\n              <div class=\"forn-group\">\n                <button type=\"submit\" class=\"btn btn-success btn-block\">เข้าสู่ระบบ</button>\n                <button\n                  type=\"button\"\n                  @click=\"onRedirectToRegister()\"\n                  class=\"btn btn-secondary btn-block\"\n                >ลงทะเบียน</button>\n              </div>\n            </form>\n          </div>\n        </div>\n      </div>\n      <div class=\"col-md-3\"></div>\n    </div>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      form: {\n        TEL: \"\",\n        PASSWORD: \"\"\n      }\n    };\n  },\n  methods: {\n    //ส่งข้อมูลไปตรวจสอบ\n    onSubmit() {\n      const response = axios\n        .post(\"api/account/login\",this.form)\n        .then(response => {\n          \n          this.$router.push(\"/\")\n        })\n        .catch(err => {\n          console.log(err.response.data);\n        });\n    },\n    onReset() {\n      this.onSubmit.rest();\n      this.form = {\n        TEL: \"\",\n        PASSWORD: \"\"\n      };\n    },\n    //ไปหน้าลงทะเบียน\n    onRedirectToRegister() {\n      this.$router.push(\"/register\");\n    }\n  }\n};\n</script>\n\n<style scoped>\n.name-system {\n  width: 80%;\n  display: block;\n  margin: auto;\n  margin-top: 15%;\n  margin-bottom: 40px;\n}\nh1 {\n  font-size: 18px;\n  font-weight: 600;\n  text-align: center;\n}\n.card-header {\n  letter-spacing: 5px;\n}\n.card-body {\n  padding-left: 15%;\n  padding-right: 15%;\n}\n.btn {\n  margin-top: 30px;\n}\n</style>"],"sourceRoot":"src/views"}]}